{
    "version": 2,
    "builds": [
        {
            "src": "web.py",
            "use": "@vercel/python"
        },
      {
            "src": "*.html",
            "use": "@vercel/templates"
        }
    ],
    "routes": [
        {
            "src": "/(*)",
            "dest": "index.py"
        }
    ]
}